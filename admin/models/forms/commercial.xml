<?xml version="1.0" encoding="utf-8"?>
<form
	addrulepath="/administrator/components/com_realestatenow/models/rules"
	addfieldpath="/administrator/components/com_realestatenow/models/fields"
>
	<fieldset name="details">
                <!-- [Fields 736] Default Fields.-->
                <!-- [Fields 737] Id Field. Type: Text (joomla)-->
                <field name="id"
                       type="text"
                       class="readonly"
                       readonly="true"
                       label="JGLOBAL_FIELD_ID_LABEL"
                       description="JGLOBAL_FIELD_ID_DESC"
                       size="10"
                       default="0" />
                <!-- [Fields 769] Date Created Field. Type: Calendar (joomla)-->
                <field name="created"
                       type="calendar"
                       label="COM_REALESTATENOW_COMMERCIAL_CREATED_DATE_LABEL"
                       description="COM_REALESTATENOW_COMMERCIAL_CREATED_DATE_DESC"
                       size="22"
                       format="%Y-%m-%d %H:%M:%S"
                       filter="user_utc" />
                <!-- [Fields 785] User Created Field. Type: User (joomla)-->
                <field name="created_by"
                       type="user"
                       label="COM_REALESTATENOW_COMMERCIAL_CREATED_BY_LABEL"
                       description="COM_REALESTATENOW_COMMERCIAL_CREATED_BY_DESC" />
                <!-- [Fields 800] Published Field. Type: List (joomla)-->
                <field name="published"
                       type="list"
                       label="JSTATUS">
                        <option value="1">JPUBLISHED</option>
                        <option value="0">JUNPUBLISHED</option>
                        <option value="2">JARCHIVED</option>
                        <option value="-2">JTRASHED</option>
                </field>
                <!-- [Fields 826] Date Modified Field. Type: Calendar (joomla)-->
                <field name="modified"
                       type="calendar"
                       class="readonly"
                       label="COM_REALESTATENOW_COMMERCIAL_MODIFIED_DATE_LABEL"
                       description="COM_REALESTATENOW_COMMERCIAL_MODIFIED_DATE_DESC"
                       size="22"
                       readonly="true"
                       format="%Y-%m-%d %H:%M:%S"
                       filter="user_utc" />
                <!-- [Fields 844] User Modified Field. Type: User (joomla)-->
                <field name="modified_by"
                       type="user"
                       label="COM_REALESTATENOW_COMMERCIAL_MODIFIED_BY_LABEL"
                       description="COM_REALESTATENOW_COMMERCIAL_MODIFIED_BY_DESC"
                       class="readonly"
                       readonly="true"
                       filter="unset" />
                <!-- [Fields 862] Access Field. Type: Accesslevel (joomla)-->
                <field name="access"
                       type="accesslevel"
                       label="JFIELD_ACCESS_LABEL"
                       description="JFIELD_ACCESS_DESC"
                       default="1"
                       required="false" />
                <!-- [Fields 882] Ordering Field. Type: Numbers (joomla)-->
                <field name="ordering"
                       type="number"
                       class="inputbox validate-ordering"
                       label="COM_REALESTATENOW_COMMERCIAL_ORDERING_LABEL"
                       description=""
                       default="0"
                       size="6"
                       required="false" />
                <!-- [Fields 901] Version Field. Type: Text (joomla)-->
                <field name="version"
                       type="text"
                       class="readonly"
                       label="COM_REALESTATENOW_COMMERCIAL_VERSION_LABEL"
                       description="COM_REALESTATENOW_COMMERCIAL_VERSION_DESC"
                       size="6"
                       readonly="true"
                       filter="unset" />
                <!-- [Fields 919] Metakey Field. Type: Textarea (joomla)-->
                <field name="metakey"
                       type="textarea"
                       label="JFIELD_META_KEYWORDS_LABEL"
                       description="JFIELD_META_KEYWORDS_DESC"
                       rows="3"
                       cols="30" />
                <!-- [Fields 928] Metadesc Field. Type: Textarea (joomla)-->
                <field name="metadesc"
                       type="textarea"
                       label="JFIELD_META_DESCRIPTION_LABEL"
                       description="JFIELD_META_DESCRIPTION_DESC"
                       rows="3"
                       cols="30" />
                <!-- [Fields 937] Dynamic Fields.-->
                <!-- [Fields 1897] Propid Field. Type: Properties. (custom)-->
                <field type="properties"
                       name="propid"
                       label="COM_REALESTATENOW_COMMERCIAL_PROPID_LABEL"
                       description="COM_REALESTATENOW_COMMERCIAL_PROPID_DESCRIPTION"
                       class="list_class"
                       multiple="false"
                       default="0"
                       required="true"
                       readonly="true"
                       button="false" />
                <!-- [Fields 1657] Currentuse Field. Type: Text. (joomla)-->
                <field type="text"
                       name="currentuse"
                       label="COM_REALESTATENOW_COMMERCIAL_CURRENTUSE_LABEL"
                       size="10"
                       maxlength="50"
                       default=""
                       class="text_area"
                       readonly="false"
                       disabled="false"
                       required="false"
                       filter="STRING"
                       message="COM_REALESTATENOW_COMMERCIAL_CURRENTUSE_MESSAGE"
                       hint="COM_REALESTATENOW_COMMERCIAL_CURRENTUSE_HINT" />
                <!-- [Fields 1657] Carryingcap Field. Type: Text. (joomla)-->
                <field type="text"
                       name="carryingcap"
                       label="COM_REALESTATENOW_COMMERCIAL_CARRYINGCAP_LABEL"
                       size="10"
                       maxlength="50"
                       default=""
                       class="text_area"
                       readonly="false"
                       disabled="false"
                       required="false"
                       filter="STRING"
                       message="COM_REALESTATENOW_COMMERCIAL_CARRYINGCAP_MESSAGE"
                       hint="COM_REALESTATENOW_COMMERCIAL_CARRYINGCAP_HINT" />
                <!-- [Fields 1657] Percentwarehouse Field. Type: Text. (joomla)-->
                <field type="text"
                       name="percentwarehouse"
                       label="COM_REALESTATENOW_COMMERCIAL_PERCENTWAREHOUSE_LABEL"
                       size="10"
                       maxlength="50"
                       default=""
                       class="text_area"
                       readonly="false"
                       disabled="false"
                       required="false"
                       filter="STRING"
                       message="COM_REALESTATENOW_COMMERCIAL_PERCENTWAREHOUSE_MESSAGE"
                       hint="COM_REALESTATENOW_COMMERCIAL_PERCENTWAREHOUSE_HINT" />
                <!-- [Fields 1657] Loadingfac Field. Type: Text. (joomla)-->
                <field type="text"
                       name="loadingfac"
                       label="COM_REALESTATENOW_COMMERCIAL_LOADINGFAC_LABEL"
                       size="10"
                       maxlength="50"
                       default=""
                       class="text_area"
                       readonly="false"
                       disabled="false"
                       required="false"
                       filter="STRING"
                       message="COM_REALESTATENOW_COMMERCIAL_LOADINGFAC_MESSAGE"
                       hint="COM_REALESTATENOW_COMMERCIAL_LOADINGFAC_HINT" />
                <!-- [Fields 1657] Bussubtype Field. Type: Text. (joomla)-->
                <field type="text"
                       name="bussubtype"
                       label="COM_REALESTATENOW_COMMERCIAL_BUSSUBTYPE_LABEL"
                       size="10"
                       maxlength="50"
                       default=""
                       class="text_area"
                       readonly="false"
                       disabled="false"
                       required="false"
                       filter="STRING"
                       message="COM_REALESTATENOW_COMMERCIAL_BUSSUBTYPE_MESSAGE"
                       hint="COM_REALESTATENOW_COMMERCIAL_BUSSUBTYPE_HINT" />
                <!-- [Fields 1657] Percentoffice Field. Type: Text. (joomla)-->
                <field type="text"
                       name="percentoffice"
                       label="COM_REALESTATENOW_COMMERCIAL_PERCENTOFFICE_LABEL"
                       size="10"
                       maxlength="50"
                       default=""
                       class="text_area"
                       readonly="false"
                       disabled="false"
                       required="false"
                       filter="STRING"
                       message="COM_REALESTATENOW_COMMERCIAL_PERCENTOFFICE_MESSAGE"
                       hint="COM_REALESTATENOW_COMMERCIAL_PERCENTOFFICE_HINT" />
                <!-- [Fields 1657] Bustype Field. Type: Text. (joomla)-->
                <field type="text"
                       name="bustype"
                       label="COM_REALESTATENOW_COMMERCIAL_BUSTYPE_LABEL"
                       size="10"
                       maxlength="50"
                       default=""
                       class="text_area"
                       readonly="false"
                       disabled="false"
                       required="false"
                       filter="STRING"
                       message="COM_REALESTATENOW_COMMERCIAL_BUSTYPE_MESSAGE"
                       hint="COM_REALESTATENOW_COMMERCIAL_BUSTYPE_HINT" />
                <!-- [Fields 1657] Netprofit Field. Type: Text. (joomla)-->
                <field type="text"
