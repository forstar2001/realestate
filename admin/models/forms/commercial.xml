                       required="false"
                       filter="STRING"
                       message="COM_REALESTATENOW_COMMERCIAL_NETPROFIT_MESSAGE"
                       hint="COM_REALESTATENOW_COMMERCIAL_NETPROFIT_HINT" />
                <!-- [Fields 1657] Returns Field. Type: Text. (joomla)-->
                <field type="text"
                       name="returns"
                       label="COM_REALESTATENOW_COMMERCIAL_RETURNS_LABEL"
                       size="10"
                       maxlength="50"
                       default=""
                       class="text_area"
                       readonly="false"
                       disabled="false"
                       required="false"
                       filter="STRING"
                       message="COM_REALESTATENOW_COMMERCIAL_RETURNS_MESSAGE"
